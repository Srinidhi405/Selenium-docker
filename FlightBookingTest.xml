<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="FlightBookingTest.xml">
    <test name="book-flight-1">
        <parameter name="noOfPassengers" value="1" />
        <parameter name="expectedCost" value="$584 USD" />
        <classes>
            <class name="com.demoaut.newtours.flightbooktest.FlightBookingTest" />
        </classes>
    </test>
    <test name="book-flight-2">
        <parameter name="noOfPassengers" value="2" />
        <parameter name="expectedCost" value="$1169 USD" />
        <classes>
            <class name="com.demoaut.newtours.flightbooktest.FlightBookingTest" />
        </classes>
    </test>
    <test name="book-flight-3">
        <parameter name="noOfPassengers" value="3" />
        <parameter name="expectedCost" value="$1753 USD" />
        <classes>
            <class name="com.demoaut.newtours.flightbooktest.FlightBookingTest" />
        </classes>
    </test>
    <test name="book-flight-4">
        <parameter name="noOfPassengers" value="4" />
        <parameter name="expectedCost" value="$2338 USD" />
        <classes>
            <class name="com.demoaut.newtours.flightbooktest.FlightBookingTest" />
        </classes>
    </test>
</suite>